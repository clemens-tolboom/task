<?php

/**
 * @file
 * Bulk export of views_default objects generated by Bulk export module.
 */

/**
 * Implements hook_views_default_views().
 */
function task_views_default_views() {
  $views = array();

  $view = new view();
  $view->name = 'task_admin_tasks_list';
  $view->description = 'Default admin tasks list.';
  $view->tag = 'task';
  $view->base_table = 'task';
  $view->human_name = 'Admin tasks list';
  $view->core = 7;
  $view->api_version = '3.0';
  $view->disabled = FALSE; /* Edit this to true to make a default view disabled initially */

  /* Display: Master */
  $handler = $view->new_display('default', 'Master', 'default');
  $handler->display->display_options['title'] = 'Tasks';
  $handler->display->display_options['use_more_always'] = FALSE;
  $handler->display->display_options['access']['type'] = 'perm';
  $handler->display->display_options['access']['perm'] = 'administer task entities';
  $handler->display->display_options['cache']['type'] = 'none';
  $handler->display->display_options['query']['type'] = 'views_query';
  $handler->display->display_options['exposed_form']['type'] = 'basic';
  $handler->display->display_options['pager']['type'] = 'full';
  $handler->display->display_options['pager']['options']['items_per_page'] = '10';
  $handler->display->display_options['style_plugin'] = 'table';
  /* Field: Task: Task ID */
  $handler->display->display_options['fields']['tid']['id'] = 'tid';
  $handler->display->display_options['fields']['tid']['table'] = 'task';
  $handler->display->display_options['fields']['tid']['field'] = 'tid';
  $handler->display->display_options['fields']['tid']['exclude'] = TRUE;
  /* Field: Task: Complete */
  $handler->display->display_options['fields']['complete']['id'] = 'complete';
  $handler->display->display_options['fields']['complete']['table'] = 'task';
  $handler->display->display_options['fields']['complete']['field'] = 'complete';
  /* Field: Task: Title */
  $handler->display->display_options['fields']['title']['id'] = 'title';
  $handler->display->display_options['fields']['title']['table'] = 'task';
  $handler->display->display_options['fields']['title']['field'] = 'title';
  $handler->display->display_options['fields']['title']['alter']['make_link'] = TRUE;
  $handler->display->display_options['fields']['title']['alter']['path'] = 'task/[tid]';
  $handler->display->display_options['fields']['title']['element_label_colon'] = FALSE;
  /* Field: Task: Author */
  $handler->display->display_options['fields']['uid']['id'] = 'uid';
  $handler->display->display_options['fields']['uid']['table'] = 'task';
  $handler->display->display_options['fields']['uid']['field'] = 'uid';
  /* Field: Task: Assignee */
  $handler->display->display_options['fields']['task_assignee']['id'] = 'task_assignee';
  $handler->display->display_options['fields']['task_assignee']['table'] = 'field_data_task_assignee';
  $handler->display->display_options['fields']['task_assignee']['field'] = 'task_assignee';
  $handler->display->display_options['fields']['task_assignee']['settings'] = array(
    'link' => 1,
  );

  /* Display: Page */
  $handler = $view->new_display('page', 'Page', 'page');
  $handler->display->display_options['path'] = 'admin/content/tasks';
  $handler->display->display_options['menu']['type'] = 'tab';
  $handler->display->display_options['menu']['title'] = 'Tasks';
  $handler->display->display_options['menu']['weight'] = '0';
  $handler->display->display_options['menu']['name'] = 'management';
  $handler->display->display_options['menu']['context'] = 0;
  $handler->display->display_options['menu']['context_only_inline'] = 0;
  $handler->display->display_options['tab_options']['type'] = 'normal';
  $handler->display->display_options['tab_options']['title'] = 'Tasks';
  $handler->display->display_options['tab_options']['weight'] = '0';
  $handler->display->display_options['tab_options']['name'] = 'management';
  $views['task_admin_tasks_list'] = $view;

  return $views;
}
